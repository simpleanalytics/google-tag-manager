const logToConsole = require('logToConsole');
const encodeUriComponent = require('encodeUriComponent');
const injectScript = require('injectScript');
const queryPermission = require('queryPermission');
const createArgumentsQueue = require('createArgumentsQueue');
const setInWindow = require('setInWindow');

const defaultHostname = 'scripts.simpleanalyticscdn.com';

const log = (message) => {
  logToConsole('Simple Analytics (GTM):', message);
};

const hostname = data.customDomain && data.customDomainToggle ? data.customDomain : defaultHostname;

const url = 'https://' + hostname;
const latestScript = url + '/latest.js';
const autoEventScript = url + '/auto-events.js';

const settings = {};

if (data.allowUrlParametersToggle && data.allowUrlParameters.length > 0) {
  settings.allowParams = data.allowUrlParameters.map((field) => field.parameter);
}
if (data.ignorePagesToggle && data.ignorePages.length > 0) {
  settings.ignorePages = data.ignorePages.map((field) => field.path);
}
if (data.hostnameToggle && data.hostname) {
  settings.hostname = data.hostname;
}
if (data.autoCollect) {
  settings.autoCollect = data.autoCollect;
}
if (data.collectDnt) {
  settings.collectDnt = data.collectDnt;
}
if (data.mode) {
  settings.mode = data.mode;
}
if (data.strictUtm) {
  settings.strictUtm = data.strictUtm;
}
if (data.ignoreMetricsToggle) {
  const list = [];
  if (data.ignoreMetricCountry) { list.push('country'); }
  if (data.ignoreMetricLanguage) { list.push('language'); }
  if (data.ignoreMetricReferrer) { list.push('referrer'); }
  if (data.ignoreMetricScreensize) { list.push('screensize'); }
  if (data.ignoreMetricScrolled) { list.push('scrolled'); }
  if (data.ignoreMetricSession) { list.push('session'); }
  if (data.ignoreMetricTimeonpage) { list.push('timeonpage'); }
  if (data.ignoreMetricUseragent) { list.push('useragent'); }
  if (data.ignoreMetricUtm) { list.push('utm'); }
  if (data.ignoreMetricViewportsize) { list.push('viewportsize'); }
  if (list.length) settings.ignoreMetrics = list;
}

setInWindow('sa_settings', settings, true);

if (data.enableEvents) {
  createArgumentsQueue('sa_event', 'sa_event.q');
}

const onSuccessDefault = () => {
  log('Loaded default script successfully.');

  // We only run data.gtmOnSuccess() when we don't use the auto events script
  if (!data.enableAutomatedEvents) {
    data.gtmOnSuccess();
  }
};

const onFailureDefault = () => {
  log('Loading default script failed.');
  data.gtmOnFailure();
};

const onSuccessAutoEvents = () => {
  log('Loaded auto events script successfully.');
  data.gtmOnSuccess();
};

const onFailureAutoEvents = () => {
  log('Loading auto events script failed.');
  data.gtmOnFailure();
};

// Check if we got permission to use the custom domain
if (queryPermission('inject_script', latestScript)) {

  // Inject default latest script
  injectScript(latestScript, onSuccessDefault, onFailureDefault, latestScript);

  // Inject auto events script
  if (data.enableAutomatedEvents) {
    if (queryPermission('inject_script', autoEventScript)) {
      injectScript(autoEventScript, onSuccessAutoEvents, onFailureAutoEvents, autoEventScript);
    } else {
      log('No permission to load automated events script (' + autoEventScript + '). Check the "Inject scripts" in the "Permissions" tab of Google Tab Manager.');
    }
  }
} else {
  if (hostname !== defaultHostname) {
    log('You need to add "https://' + hostname + '/*" to "Inject scripts" in the "Permissions" tab of Google Tab Manager');
  } else {
    log('No permission to load default script (' + latestScript + '). Check the "Inject scripts" in the "Permissions" tab of Google Tab Manager.');
  }
  data.gtmOnFailure();
}


